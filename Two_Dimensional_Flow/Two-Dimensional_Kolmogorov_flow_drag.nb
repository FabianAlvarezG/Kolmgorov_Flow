     RowBox[{"Conjugate", "[", 
      RowBox[{"A", " ", "\[Psi]\[Alpha]c"}], "]"}]}], ",", 
    RowBox[{"Assumptions", "\[Rule]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[Alpha]", " ", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"x", " ", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"y", " ", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"Rc", " ", "\[Element]", "Reals"}], ",", " ", 
       RowBox[{"Rc0", " ", "\[Element]", "Reals"}]}], "}"}]}]}], 
   "]"}]}]], "Input",
