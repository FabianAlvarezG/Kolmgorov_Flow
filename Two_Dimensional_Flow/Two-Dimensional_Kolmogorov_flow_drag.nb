Cell[BoxData[
 RowBox[{
  RowBox[{"A", " ", 
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", "x", " ", "\[Alpha]"}]], " ", 
   RowBox[{"(", 
