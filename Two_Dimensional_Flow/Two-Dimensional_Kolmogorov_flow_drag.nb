 RowBox[{"\[Psi]", "=", 
  RowBox[{
   RowBox[{"A", " ", 
    SubscriptBox["\[Psi]", "\[Alpha]"]}], "+", 
   RowBox[{
    SuperscriptBox["A", "*"], 
    SubsuperscriptBox["\[Psi]", "\[Alpha]", "*"]}], "+", 
   RowBox[{"W", 
    RowBox[{"(", 
     RowBox[{"A", ",", 
      SuperscriptBox["A", "*"]}], ")"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["\[PartialD]", "t"], "A"}], "=", 
  RowBox[{
   RowBox[{
    SuperscriptBox["f", 
     RowBox[{"[", "2", "]"}]], "+", 
    SuperscriptBox["f", 
     RowBox[{"[", "3", "]"}]], "+"}], "..."}]}], "\[IndentingNewLine]", 
 RowBox[{"W", " ", "=", " ", 
  RowBox[{
   RowBox[{
    SuperscriptBox["W", 
     RowBox[{"[", "2", "]"}]], "+", 
    SuperscriptBox["W", 
